{
  "name": "DSMR Datalogger",
  "version": "0.1.0",
  "slug": "dsmr_datalogger",
  "description": "The datalogger only option of DSMR (https://dsmr-reader.readthedocs.io)",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "url": "https://github.com/sanderdw/hassio-addons",
  "startup": "system",
  "boot": "auto",
  "auto_uart": true,
  "options": {
    "DSMR_USB_PORT": "/dev/ttyUSBX",
    "DSMR_API_URL": "https://<YOUR_DSMR_HOST>/api/v1/datalogger/dsmrreading",
    "DSMR_API_KEY": "<YOUR_API_KEY>"
  },
  "schema": {
    "DSMR_USB_PORT": "str",
    "DSMR_API_URL": "str",
    "DSMR_API_KEY": "str"
  }
}